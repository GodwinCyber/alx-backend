ould set in Redis the value for the key schoolName\n// It should display a confirmation message using redis.print\n// displaySchoolValue:\n// It accepts one argument schoolName.\n// It should log to the console the value for the key passed as argument\n// At the end of the file, call:\n// displaySchoolValue('Holberton');\n// setNewSchool('HolbertonSanFrancisco', '100');\n// displaySchoolValue('HolbertonSanFrancisco');\n\nimport { createClient, print } from \"redis\";\n\nconst client = createClient();\n\nclient.on('error', (err) => {\n    console.log('Redis client not connected to the server:', err.toString());\n});\nclient.on('connect', () => {\n    console.log('Redis client connected to the server');\n});\n\nconst setNewSchool = (schoolName, value) => {\n    client.set(schoolName, value, print);\n}\n\nconst displaySchoolValue = (schoolName) => {\n    client.get(schoolName, (err, reply) => {\n        console.log(reply);\n    });\n}\ndisplaySchoolValue('Holberton');\nsetNewSchool('HolbertonSanFrancisco', '100');\ndisplaySchoolValue('HolbertonSanFrancisco');\n"],"mappings":";;AAaA,IAAAA,MAAA,GAAAC,OAAA;AAbA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAIA,IAAMC,MAAM,GAAG,IAAAC,mBAAY,EAAC,CAAC;AAE7BD,MAAM,CAACE,EAAE,CAAC,OAAO,EAAE,UAACC,GAAG,EAAK;EACxBC,OAAO,CAACC,GAAG,CAAC,2CAA2C,EAAEF,GAAG,CAACG,QAAQ,CAAC,CAAC,CAAC;AAC5E,CAAC,CAAC;AACFN,MAAM,CAACE,EAAE,CAAC,SAAS,EAAE,YAAM;EACvBE,OAAO,CAACC,GAAG,CAAC,sCAAsC,CAAC;AACvD,CAAC,CAAC;AAEF,IAAME,YAAY,GAAG,SAAfA,YAAYA,CAAIC,UAAU,EAAEC,KAAK,EAAK;EACxCT,MAAM,CAACU,GAAG,CAACF,UAAU,EAAEC,KAAK,EAAEE,YAAK,CAAC;AACxC,CAAC;AAED,IAAMC,kBAAkB,GAAG,SAArBA,kBAAkBA,CAAIJ,UAAU,EAAK;EACvCR,MAAM,CAACa,GAAG,CAACL,UAAU,EAAE,UAACL,GAAG,EAAEW,KAAK,EAAK;IACnCV,OAAO,CAACC,GAAG,CAACS,KAAK,CAAC;EACtB,CAAC,CAAC;AACN,CAAC;AACDF,kBAAkB,CAAC,WAAW,CAAC;AAC/BL,YAAY,CAAC,uBAAuB,EAAE,KAAK,CAAC;AAC5CK,kBAAkB,CAAC,uBAAuB,CAAC","ignoreList":[]}},"mtime":1722352142676}}